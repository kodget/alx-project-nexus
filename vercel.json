{
  "version": 2,
  "builds": [
    {
      "src": "distinct-pattern/package.json",
      "use": "@vercel/next",
      "config": {
        "projectSettings": {
          "framework": "nextjs"
        }
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/distinct-pattern/$1"
    }
  ]
}